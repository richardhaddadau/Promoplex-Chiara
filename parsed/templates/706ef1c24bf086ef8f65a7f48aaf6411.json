{
  "components/common/quick-search": "{{{ snippet \"forms_search\"}}}\n<a class=\"modal-close mobileOnly\" aria-label=\"{{lang 'common.close'}}\" data-drop-down-close role=\"button\">\n    <span aria-hidden=\"true\">&#215;</span>\n</a>\n<form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query{{idPrefix}}\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\"\n                {{#if forms.search.query}} value=\"{{forms.search.query}}\"{{/if}}>\n        </div>\n    </fieldset>\n</form>\n<div class=\"container\">\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n"
}