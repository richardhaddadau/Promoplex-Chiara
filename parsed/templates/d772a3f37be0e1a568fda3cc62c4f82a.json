{
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}{{#if idSuffix}}{{../idSuffix}}{{/if}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action{{#unless noDepthMaxClass}} navPages-action-depth-max{{/unless}}\" href=\"{{url}}\">{{{lang 'category.view_all.name' category=name}}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#unless ../../noDepthMaxClass}} navPages-action-depth-max{{/unless}} has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}{{#if ../../idSuffix}}{{../../../idSuffix}}{{/if}}\"\n                        {{#if ../../theme_settings.navigation_hover}}\n                            data-collapsible-disabled-breakpoint=\"large\"\n                            data-collapsible-disabled-state=\"open\"\n                            data-collapsible-enabled-state=\"closed\"\n                        {{/if}}\n                    >\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown idSuffix=../../idSuffix noDepthMaxClass=../../noDepthMaxClass theme_settings=../../theme_settings}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n"
}