{
  "components/chiara/sections/recent-blog": "{{#block \"chiara-recent-blog\"}}\n{{#and settings.blog_enabled blog.recent_posts}}\n<div class=\"chiara-section chiara-section--recentBlog\">\n    <div class=\"chiara-recentBlog{{#if theme_settings.recentBlog_type}} chiara-recentBlog--{{theme_settings.recentBlog_type}}{{/if}}\">\n        {{#if theme_settings.recentBlog_heading}}\n            <h2 class=\"page-heading\">{{theme_settings.recentBlog_heading}}</h2>\n        {{/if}}\n        {{#if theme_settings.recentBlog_subheading}}\n            <p class=\"page-subheading\">{{theme_settings.recentBlog_subheading}}</p>\n        {{/if}}\n        <div class=\"chiara-recentBlog-posts\">\n            {{#each blog.recent_posts}}\n                {{> components/blog/post post=this settings=../settings title_tag=\"h3\"}}\n            {{/each}}\n        </div>\n        <p class=\"chiara-recentBlog-button\"><a href=\"/blog/\" class=\"button\">View All Posts</a></p>\n    </div>\n</div>\n{{/and}}\n{{/block}}\n",
  "components/blog/post": "<article class=\"blog{{#if single}} blog--single{{/if}}\">\n    <div class=\"blog-post-figure\">\n        {{#if post.thumbnail}}\n            <figure class=\"blog-thumbnail\">\n                <a href=\"{{post.url}}\">\n                    <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{#if large_image}}{{getImage post.thumbnail 'blog_post_size'}}{{else}}{{getImage post.thumbnail 'blog_size'}}{{/if}}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\n                </a>\n            </figure>\n        {{/if}}\n    </div>\n\n    <div class=\"blog-post-body\">\n        <header class=\"blog-header\">\n            {{#if title_tag}}\n                <{{title_tag}} class=\"blog-title\">{{post.title}}</{{title_tag}}>\n            {{else if single}}\n                <h1 class=\"blog-title\">{{post.title}}</h1>\n            {{else}}\n                <h2 class=\"blog-title\"><a href=\"{{post.url}}\">{{post.title}}</a></h2>\n            {{/if}}\n            <p class=\"blog-date\">{{#if post.author}}{{lang 'blog.posted_by' name=post.author}} on {{/if}}{{post.date_published}}</p>\n        </header>\n\n        <div class=\"blog-post\">\n            {{#if single}}\n                {{{region name=\"blog_post_content--global\"}}}\n                {{{region name=\"blog_post_content\"}}}\n            {{/if}}\n            {{#if post.body}}\n                {{{post.body}}}\n            {{else}}\n                {{{post.summary}}}\n                {{#if post.show_read_more}}\n                    &hellip; <a href=\"{{url}}\">{{lang 'blog.read_more'}}</a>\n                {{/if}}\n            {{/if}}\n        </div>\n\n\n        {{#if show_tags}}\n            {{#if post.tags}}\n                <ul class=\"tags\">\n                    {{#each post.tags}}\n                    <li class=\"tag\">\n                        <a href=\"{{url}}\">#{{name}}</a>\n                    </li>\n                    {{/each}}\n                </ul>\n            {{/if}}\n        {{/if}}\n\n        {{#if show_share}}\n            {{>components/common/share url=post.url}}\n        {{/if}}\n\n    </div>\n</article>\n",
  "components/common/share": "\n{{#or settings.add_this.buttons settings.facebook_like_button.enabled}}\n{{assignVar 'encodedUrl' (encodeURI url)}}\n{{assignVar 'encodedTitle' (encodeURI head.title)}}\n<div class=\"addthis_toolbox\">\n    {{#if settings.add_this.buttons}}\n        <ul class=\"socialLinks socialLinks--colors\">\n            {{#each settings.add_this.buttons}}\n                <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                    {{#if service '===' 'facebook'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                    title=\"{{{capitalize service}}}\"\n                    href=\"https://facebook.com/sharer/sharer.php?u={{getVar 'encodedUrl'}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-facebook\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'email'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                    title=\"{{{capitalize service}}}\"\n                    href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\n                    target=\"_self\"\n                    rel=\"noopener\"\n                    {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-envelope\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'print'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                    title=\"{{{capitalize service}}}\"\n                    onclick=\"window.print();return false;\"\n                    {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-print\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'twitter'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                    href=\"https://twitter.com/intent/tweet/?text={{getVar 'encodedTitle'}}&amp;url={{getVar 'encodedUrl'}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"{{{capitalize service}}}\"\n                    {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-twitter\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'linkedin'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                    title=\"{{{capitalize service}}}\"\n                    href=\"https://www.linkedin.com/shareArticle?mini=true&amp;url={{getVar 'encodedUrl'}}&amp;title={{getVar 'encodedTitle'}}&amp;summary={{getVar 'encodedTitle'}}&amp;source={{getVar 'encodedUrl'}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-linkedin\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'pinterest'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                    title=\"{{{capitalize service}}}\"\n                    href=\"https://pinterest.com/pin/create/button/?url={{getVar 'encodedUrl'}}&amp;description={{getVar 'encodedTitle'}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-pinterest\"/>\n                        </svg>\n                    </a>\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n    {{/if}}\n    {{#if settings.facebook_like_button.enabled}}\n        <iframe\n            class=\"facebook{{#if settings.facebook_like_button.verb '===' 'recommend'}}Recommend{{else}}Like{{/if}}Btn\"\n            style=\"border:none; overflow:hidden; margin: 0; padding: 0; height: 20px; margin-top: .75rem; max-width: 100%\"\n            src=\"https://www.facebook.com/plugins/like.php?href={{settings.facebook_like_button.href}}&amp;layout=button_count{{#if settings.facebook_like_button.verb '===' 'recommend'}}&amp;action=recommend{{/if}}&amp;colorscheme=light&amp;height=20\"\n            scrolling=\"no\"\n            frameborder=\"0\"\n            allowTransparency=\"true\">\n        </iframe>\n    {{/if}}\n</div>\n{{/or}}\n\n"
}