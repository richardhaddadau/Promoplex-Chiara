{
  "components/common/wishlist-dropdown": "<form action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist form-action\" data-wishlist-add method=\"post\">\n    <a aria-controls=\"wishlist-dropdown-{{product.id}}\" aria-expanded=\"false\" class=\"button dropdown-menu-button\" data-dropdown=\"wishlist-dropdown-{{product.id}}\">\n        <span class=\"is-srOnly\">{{lang 'account.wishlists.add_item'}}</span>\n        <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-heart\" /></svg></i>\n    </a>\n    <ul aria-hidden=\"true\" class=\"dropdown-menu wishlist-dropdown\" data-dropdown-content id=\"wishlist-dropdown-{{product.id}}\" tabindex=\"-1\">\n        {{#if customer.wishlists}} {{#each customer.wishlists}}\n        <li>\n            <input class=\"button {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\" formaction=\"{{this.add_url}}&product_id={{../product.id}}\"\n                type=\"submit\" value=\"{{this.name}}\">\n        </li>\n        {{/each}} {{else}}\n        <li>\n            <input class=\"button\" type=\"submit\" value=\"{{lang 'account.wishlists.add_to_default'}}\">\n        </li>\n        {{/if}}\n        <li>\n            <a data-wishlist class=\"button\" href=\"/wishlist.php?action=addwishlist&product_id={{product.id}}\">{{lang 'account.wishlists.create_new'}}</a>\n        </li>\n    </ul>\n</form>\n"
